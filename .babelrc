{
    "presets":  [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["@babel/plugin-transform-runtime", {
            "corejs": {
              "version": 3,
              "proposals": true
            },
            "useESModules": true
        }],
        "@babel/plugin-syntax-dynamic-import"
    ],
    "sourceType": "unambiguous"
}